{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-path-concat": "off",
    "no-unused-vars": "warn",
    "space-before-function-paren": "off",
    "no-var": "error"
  }
}
